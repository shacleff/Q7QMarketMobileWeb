<div class="rechargeRecordDetail cashDetail assignTradeDetail">
  <!--<Header
    [title]="headerTitle"
    (onVoted)="back($event)">
    >
  </Header>-->
  <div class="header">
    <i feedButton class="leftSwordIcon" (click)="hide()"></i>
    <h3 class="headerTitle">{{topTitle}}</h3>
  </div>
  <div class="rchRecBase mt80Rem">
    <div class="rchType tc">
      <img src="../../../assets/images/trade_detial_wood_icon.png" alt="">
      &nbsp;{{item.proName}}
    </div>
    <p class="rchNum tc">
      {{item.cnt}}
    </p>
    <p class="rchSratus tc">
      {{item.statusText}}
    </p>
  </div>
  <div class="rchInfoTable">
    <div class="grayBgBar"></div>
    <table class="WrapB tableT">
      <tr>
        <td>赠送人</td>
        <td>{{item.suName}}</td>
      </tr>
      <tr>
        <td>接收人</td>
        <td>{{item.tuName}}</td>
      </tr>
      <tr>
        <td>手续费</td>
        <td>{{item.charge}}</td>
      </tr>
      <tr>
        <td>索取金币</td>
        <td>{{item.amt}}</td>
      </tr>
      <tr>
        <td>赠送时间</td>
        <td>{{item.createTime|date:"yyyy/M/d HH:mm:ss"}}</td>
      </tr>
    </table>
  </div>
  <div class="resolveBtn WrapB">
    <div feedButton class="btnItem confirm" (click)="confirmOrder(item.id,item.cnt,item.amt,item.proName)">
      确定
    </div>
    <div feedButton class="btnItem cancel" (click)="cancelOrder(item.id)">
      撤销
    </div>
  </div>
</div>
